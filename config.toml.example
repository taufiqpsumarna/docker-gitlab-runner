concurrent = 3

[[runners]]
  name = "gitlab-runners-0"
  url = "https://git.example.com"
  token = "<REPLACE_WITH_YOUR_GITLAB_RUNNER_REGISTRATION_TOKEN>"
  limit = 0
  executor = "docker"
  builds_dir = ""
  [runners.docker]
    host = ""
    image = "docker:24.0.6-dind-alpine3.18"
    privileged = false
    disable_cache = false
    cache_dir = "/cache"
    volumes = ["/var/run/docker.sock:/var/run/docker.sock", "/cache"]

